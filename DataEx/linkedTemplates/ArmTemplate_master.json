{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataEx"
		},
		"CSV_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CSV'"
		},
		"Parquet_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Parquet'"
		},
		"Parquet_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://strgkathelin.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataEx_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CSV_connectionString": {
						"value": "[parameters('CSV_connectionString')]"
					},
					"Parquet_accountKey": {
						"value": "[parameters('Parquet_accountKey')]"
					},
					"Parquet_properties_typeProperties_url": {
						"value": "[parameters('Parquet_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}